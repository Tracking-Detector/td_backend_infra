package components

templ Empty(title, caption string) {
	<div class="flex-grow flex-1 flex flex-col items-center justify-center">
		<div class="flex flex-col items-center justify-center align-center">
				@QuestionIcon()
				<h1 class="text-2xl font-bold mx-3">{title}</h1>
				<p class="text-lg text-gray-600">{caption}</p>
		</div>
	</div>
}
